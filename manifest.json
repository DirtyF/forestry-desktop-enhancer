{
  "name": "Forestry desktop enhancer",
  "version": "1.0.0",
  "description": "Enhace forestry web for desktop",
  "homepage_url": "https://github.com/NickCis/forestry-desktop-enhancer/",
  "manifest_version": 2,
  "background": {},
  "icons": {
    "16": "icons/icon-16.png",
    "32": "icons/icon-32.png",
    "48": "icons/icon-48.png",
    "128": "icons/icon-128.png"
  },
  "content_scripts": [
    {
      "matches": ["https://app.forestry.io/sites/*"],
      "js": ["dist/plugin.js"],
      "css": ["dist/styles.css"]
    }
  ]
}
